{
  "name": "@jetbrains/logos",
  "version": "1.4.24",
  "description": "JetBrains logos and branding materials",
  "author": "JetBrains",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "index.js",
  "files": [
    "*/*.{svg,ico,js}",
    "*/!(boxshot).png",
    "logos.js",
    "react.js",
    "LICENSE.txt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JetBrains/logos.git"
  },
  "keywords": [
    "jetbrains",
    "branding",
    "logos"
  ],
  "devDependencies": {
    "@svgr/core": "^5.4.0",
    "@svgr/plugin-jsx": "^5.4.0",
    "@svgr/plugin-prettier": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "archiver": "^3.0.0",
    "archiver-promise": "^1.0.0",
    "bluebird": "^3.5.5",
    "buble": "^0.20.0",
    "case": "^1.6.3",
    "cheerio": "^0.22.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "fs-extra": "^8.0.1",
    "glob-all": "^3.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "standard-version": "^8.0.1",
    "svg2png": "^4.1.1",
    "svgo": "^1.2.2"
  },
  "scripts": {
    "build": "scripts/prepare.sh && npm run build:png && npm run build:react-components",
    "build:assets": "node scripts/generate-assets.js",
    "build:png": "node scripts/generate-logos.js && scripts/optimize.sh",
    "build:react-components": "node scripts/generate-react-components.js",
    "prerelease:ci": "standard-version && npm run build",
    "release:ci": "git push --follow-tags origin master && cd dist/web && npm publish"
  }
}
